{
  "navigationFallback": {
    "rewrite": "/00-README.html"
  },
  "routes": [
    {
      "route": "/",
      "redirect": "/login.html",
      "statusCode": 302
    },
    {
      "route": "/login.html",
      "headers": {
        "cache-control": "no-store, no-cache, must-revalidate, max-age=0"
      }
    },
    {
      "route": "/auth-check.js",
      "headers": {
        "cache-control": "no-cache"
      }
    }
  ],
  "mimeTypes": {
    ".html": "text/html",
    ".css": "text/css",
    ".js": "text/javascript"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=3600"
  }
}
